{
  "name": "adry",
  "version": "1.0.0",
  "description": "Housekeeper hiring platform for the Philippines",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:coverage": "turbo run test -- --coverage",
    "test:e2e": "turbo run test:e2e",
